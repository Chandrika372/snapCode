public class Solution {
    public int solve(int A, int B, int C, int D) 
    {
        if((A == B && C == D) && (A != C))
        {
            return 1;
        }
        else if((A == C && B == D) && (A != B))
        {
            return 1;
        }
        else if((A == D && B == C) && (A != B))
        {
            return 1;
        }
        return 0;
    }
}
