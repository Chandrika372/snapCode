public class Solution {
    public ArrayList<Integer> solve(ArrayList<Integer> A, int B) {
        Collections.sort(A);
        int j = A.size() - 1, i = 0;
        ArrayList<Integer> res = new ArrayList<Integer>();
        
        while(i != B){
            res.add(A.get(j));
            j--;
            i++;
        }
        return res;
    }
}
